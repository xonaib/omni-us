{
  "name": "omni-us",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "test": "ng test",
    "test-coverage": "ng test --no-watch --code-coverage",
    "test-lib": "ng test design-lib",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "add-design-module": "ng g module --project=design-lib",
    "int:extract": "ng xi18n --output-path locale",
    "start:fr": "ng serve --configuration=fr",
    "build:client-and-server-bundles-i18n": "ng run omni-us:build:production && omni-us:build:production-fr ",
    "build:client-and-server-bundles": "ng build --prod && ng run omni-us:build:production",
    "build:i18n-ssr": "npm run build:client-and-server-bundles-i18n && npm run compile:server",
    "serve:ssr": "node dist/server",
    "run-prod": "npm run build:i18n-ssr && npm run serve:ssr",
    "package-lib": "npm run build-lib && npm run npm_pack",
    "showcase-deploy": "ng build --prod --base-href=\"https://xonaib.github.io/omni-us/\" && ngh --dir=dist/omni-us --no-silent"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/localize": "^9.1.7",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "rxjs": "~6.5.5",
    "rxjs-operators": "^1.1.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular-devkit/build-ng-packagr": "~0.803.25",
    "@angular/cli": "~8.3.26",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}
